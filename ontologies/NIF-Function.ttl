BIRNLEX:1874 a owl:Class ;
    rdfs:label "Memory function"^^xsd:string ;
    skos:prefLabel "Memory function"^^xsd:string ;
    NIFRID:synonym "Memory"^^xsd:string ;
    rdfs:subClassOf BIRNLEX:1864,
        [ a owl:Restriction ;
            owl:onProperty hasPart: ;
            owl:allValuesFrom [ a owl:Class ;
                    owl:unionOf (
                            BIRNLEX:1810
                            BIRNLEX:1825
                            BIRNLEX:1827 ) ] ] ;
    skos:editorialNote "Note the has_integral_part restriction helps to support the variety of memory types - e.g., verbal memory, motor memory, sense memory, etc..  It isn't truly correct to say a memory instance includes a behavioral activity as an integral part.  It would be more correct to say a memory instance (measured through some assessment technique) ENCODES or somehow includes a link to a particular behavioral activity (BB)."^^xsd:string ;
    NIFRID:createdDate "2007-03-06"^^xsd:string ;
    NIFRID:modifiedDate "August 24, 2009"^^xsd:string ;
    NIFRID:hasBirnlexCurator NIFRID:Bill_Bug ;
    NIFRID:hasCurationStatus NIFRID:uncurated .

