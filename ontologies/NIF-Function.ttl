BIRNLEX:1911 a owl:Class ;
    rdfs:label "Deductive reasoning activity"^^xsd:string ;
    skos:prefLabel "Deductive reasoning activity"^^xsd:string ;
    rdfs:subClassOf BIRNLEX:1906 ;
    NIFRID:createdDate "2007-03-06"^^xsd:string ;
